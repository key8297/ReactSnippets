		"prefix": "ctor",
		"body": [
			"    constructor(props) {",
			"        super(props);",
			"        this.state = {",
			"            $1",
			"        };",
			"    }"
		],
		"description": "Constructor"
	},
	"SetState": {
		"prefix": "ss",
		"body": [
			"this.setState((prevState, props) => ({$1});"
		],
		"description": "SetState"
	},
