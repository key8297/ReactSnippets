			"    container: {",
			"        flex: 1,",
			"        justifyContent: 'center',",
			"        alignItems: 'center',",
			"        backgroundColor: '#F5FCFF',",
			"    },",
			"    s1: {",
			"        justifyContent: 'flex-start',",
			"        alignItems: 'center',",
			"        backgroundColor: '#F5FCFF',",
			"    }    ",
			"});"
		],
		"description": "Create Style"
	},
	"ctrl1": {
		"prefix": "cc",
		"body": [
			"<${1|View,Text,Button|} />"
		],
		"description": "Component with no end tag"
