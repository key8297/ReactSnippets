		"prefix": "exfunc",
		"body": [
			"export function $1($2){",
			"}  "
		],
		"description": "Export function"
	},
	"Export Component": {
		"prefix": "excomp",
		"body": [
			"export default class $1 extends Component {",
			"    render(){",
			"        return(",
			"        ); ",
			"    }",
			"}"
		],
		"description": "Export Component"
	},
